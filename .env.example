# AWS Configuration (optional - can use AWS CLI default profile)
AWS_REGION=us-east-1
AWS_PROFILE=default

# MCP Server Configuration
MCP_SERVER_NAME=mcp-devops-automation
MCP_SERVER_VERSION=1.0.0

# Default Configuration
DEFAULT_OUTPUT_DIR=devops-generated
DEFAULT_DEPLOYMENT_STRATEGY=gitops
DEFAULT_CICD_PROVIDER=github-actions

# Feature Flags
ENABLE_MONITORING=true
ENABLE_LOGGING=true
ENABLE_COST_ESTIMATION=true
